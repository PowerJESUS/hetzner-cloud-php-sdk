  "require-dev": {
    "phpunit/phpunit": "6.5",
    "phpdocumentor/phpdocumentor": "2.*",
    "brianium/paratest": "1.*"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Lukas Kämmerling",
      "email": "kontakt@lukas-kaemmerling.de"
    },
    {
      "name": "Balázs Otakomaiya",
      "email": "balazs@vervedev.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "LKDev\\HetznerCloud\\": "src/"
    }
